                                    orientation: "vertical"
                                    BoxLayout:
                                        orientation: "horizontal"
                                        Label:
                                            text_size: self.width, None
                                            halign: "left"
                                            text: "Neutral"
                                            font_size: sp(12)
                                            bold: True
                                        Label:
                                            text_size: self.width, None
                                            halign: "right"
                                            text: f"{root.neutral_percent:.2f}%"
                                            font_size: sp(12)
                                            bold: True
                                    Widget:
                                        id: neutral_bar
                                        canvas.before:
                                            Color:
                                                rgb: (17/255,46/255,83/255)
                                            RoundedRectangle:
                                                size: self.size
                                                pos: self.pos


