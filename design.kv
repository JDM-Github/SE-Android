                                    Label:
                                        text: root.subcategory_name
                                        font_size: sp(10)
                                        bold: True
                                        text_size: self.width * 0.9, None

                                AsyncImageButton:
                                    size_hint_x: None
                                    width: 30
                                    source: "Assets/arrow.png"
                                    color: "F2C15F"
